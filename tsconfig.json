{
  "extends": "@nyx_lyb3ra/tsconfig-gnome",
  "exclude": ["builddir", "_build", "node_modules"],
  "compilerOptions": {
    "module": "preserve",
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.ts",
    "env.d.ts"
  ]
}
